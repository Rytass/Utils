{
  "name": "rytass",
  "author": "Rytass",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "clean": "lerna run clean && lerna clean",
    "build:clean": "lerna run build:clean --stream --parallel",
    "build": "lerna run build --stream --sort",
    "lint:ts": "eslint . --ext .ts",
    "lint": "npm run lint:ts",
    "test:clean": "rm -rf ./coverage",
    "test:coverage": "jest --coverage",
    "test": "jest",
    "tsnode": "node --loader ts-node/esm",
    "release": "lerna version patch && npm run build && lerna publish from-package"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/supertest": "^2.0.12",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "fs-extra": "^10.0.1",
    "glob": "^7.2.0",
    "jest": "^27.5.1",
    "lerna": "^4.0.0",
    "lint-staged": "^12.3.7",
    "rollup": "^2.70.1",
    "rollup-plugin-typescript2": "^0.31.2",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  }
}
