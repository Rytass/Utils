{
  "name": "rytass",
  "author": "Rytass",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "clean": "nx run-many -t clean",
    "build:clean": "nx run-many -t build:clean",
    "build": "nx run-many -t build",
    "lint:ts": "eslint . --ext .ts",
    "lint": "npm run lint:ts",
    "test:clean": "rm -rf ./coverage",
    "test:coverage": "jest --coverage -w 1",
    "test": "jest -w 1",
    "prepare": "husky install",
    "release": "lerna version patch && nx run-many -t build && lerna publish from-package --contents lib"
  },
  "devDependencies": {
    "@commitlint/cli": "^18",
    "@commitlint/config-conventional": "^18",
    "@commitlint/config-lerna-scopes": "^18",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.3.10",
    "@nestjs/graphql": "^12.2.0",
    "@nestjs/testing": "^10.3.10",
    "@nestjs/typeorm": "^10.0.2",
    "@nrwl/nx-cloud": "^19.0.0",
    "@nx/js": "^19.5.2",
    "@swc/core": "^1.7.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "argon2": "^0.40.3",
    "axios": "^1.7.2",
    "cz-conventional-changelog": "^3.3.0",
    "debug": "^4.3.5",
    "eslint": "^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-import": "^2.29.1",
    "fs-extra": "^11.2.0",
    "glob": "^11.0.0",
    "graphql": "^16.9.0",
    "husky": "^9.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "lerna": "^8.1.7",
    "lint-staged": "^15.2.7",
    "nx": "^19.5.2",
    "parse-multipart-data": "^1.5.0",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "rollup": "^4.19.0",
    "rollup-plugin-swc3": "^0.11.2",
    "rxjs": "^7.8.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.3",
    "typeorm": "^0.3.20",
    "typescript": "^5.5.4"
  },
  "packageManager": "yarn@4.3.1"
}
