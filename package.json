    "lint": "nx run-many -t lint --parallel=8",
    "test:clean": "rm -rf ./coverage",
    "test:coverage": "jest --coverage --passWithNoTests",
    "test": "nx run-many -t test --parallel=8",
    "test:watch": "nx run-many -t test:watch --parallel=8",
    "prepare": "husky install",
    "release": "lerna version patch && nx run-many -t build --parallel=8 && lerna publish from-package --contents lib",
    "docs": "cd docs && npm start",
    "docs:build": "node scripts/build-docs.cjs",
    "storybook": "storybook dev -p 6006",
