{
  "name": "@rytass/wms-base-nestjs-module",
  "version": "0.1.1",
  "description": "Rytass Warehouse Management System Module",
  "keywords": [
    "rytass",
    "wms",
    "nestjs"
  ],
  "homepage": "https://github.com/Rytass/Utils#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rytass/Utils.git"
  },
  "scripts": {
    "clean": "node ../../scripts/clean-build.cjs",
    "build": "npm run clean && node ../../scripts/build.cjs",
    "release": "lerna version patch && npm run build && lerna publish from-package",
    "lint": "npx eslint . --ext .ts,.tsx --fix",
    "test": "npx jest --config ../../jest.config.js --testPathPatterns=packages/wms-base-nestjs-module --passWithNoTests",
    "test:coverage": "npx jest --coverage --config ../../jest.config.js --testPathPatterns=packages/wms-base-nestjs-module --passWithNoTests"
  },
  "bugs": {
    "url": "https://github.com/Rytass/Utils/issues"
  },
  "devDependencies": {
    "sqlite3": "^5.1.7"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}
