{
  "name": "@rytass/cms-base-nestjs-module",
  "version": "0.2.5",
  "description": "Rytass Content Management System NestJS Base Module",
  "keywords": [
    "rytass",
    "cms",
    "nestjs"
  ],
  "author": "Chia Yu Pai <fantasyatelier@gmail.com>",
  "homepage": "https://github.com/Rytass/Utils#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rytass/Utils.git"
  },
  "scripts": {
    "clean": "node ../../scripts/clean-build.cjs",
    "build": "npm run clean && node ../../scripts/build.cjs",
    "release": "lerna version patch && npm run build && lerna publish from-package",
    "lint": "npx eslint . --ext .ts,.tsx --fix",
    "test": "npx jest --config ../../jest.config.js --testPathPatterns=packages/cms-base-nestjs-module --passWithNoTests",
    "test:coverage": "npx jest --coverage --config ../../jest.config.js --testPathPatterns=packages/cms-base-nestjs-module --passWithNoTests"
  },
  "bugs": {
    "url": "https://github.com/Rytass/Utils/issues"
  },
  "peerDependencies": {
    "@nestjs/common": "*",
    "@nestjs/typeorm": "*",
    "@node-rs/jieba": "*",
    "@quadrats/core": "*",
    "typeorm": "*"
  },
  "dependencies": {
    "dataloader": "^2.2.3",
    "lru-cache": "^11.1.0"
  }
}
