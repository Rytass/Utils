{
  "name": "@rytass/cms-react-components",
  "version": "0.2.1",
  "description": "Rytass Content Management System React Components",
  "keywords": [
    "rytass",
    "cms",
    "react"
  ],
  "author": "Cheng Min Ting <ting@rytass.com>",
  "homepage": "https://github.com/Rytass/Utils#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rytass/Utils.git"
  },
  "scripts": {
    "clean": "node ../../scripts/clean-build.cjs",
    "build": "npm run clean && node ../../scripts/build.cjs",
    "release": "lerna version patch && npm run build && lerna publish from-package",
    "lint": "npx eslint src --ext .ts,.tsx --fix",
    "test": "npx jest --config ../../jest.config.js --testPathPatterns=packages/cms-react-components --passWithNoTests",
    "test:coverage": "npx jest --coverage --config ../../jest.config.js --testPathPatterns=packages/cms-react-components --passWithNoTests"
  },
  "bugs": {
    "url": "https://github.com/Rytass/Utils/issues"
  },
  "peerDependencies": {
    "@mezzanine-ui/core": "*",
    "@mezzanine-ui/icons": "*",
    "@mezzanine-ui/react": "*",
    "@mezzanine-ui/system": "*",
    "dayjs": "*",
    "lodash": "*",
    "react": "*",
    "react-dom": "*",
    "react-hook-form": "*"
  }
}
