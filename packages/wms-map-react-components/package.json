{
  "name": "@rytass/wms-map-react-components",
  "version": "0.1.0",
  "description": "Rytass Warehouse Management System Map React Components",
  "keywords": [
    "rytass",
    "wms",
    "react",
    "map",
    "warehouse"
  ],
  "homepage": "https://github.com/Rytass/Utils#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rytass/Utils.git"
  },
  "scripts": {
    "clean": "node ../../scripts/clean-build.cjs",
    "build": "npm run clean && node ../../scripts/build.cjs",
    "release": "lerna version patch && npm run build && lerna publish from-package",
    "lint": "npx eslint src --ext .ts,.tsx --fix",
    "test": "npx jest --config ../../jest.config.js --testPathPatterns=packages/wms-map-react-components --passWithNoTests",
    "test:coverage": "npx jest --coverage --config ../../jest.config.js --testPathPatterns=packages/wms-map-react-components --passWithNoTests"
  },
  "bugs": {
    "url": "https://github.com/Rytass/Utils/issues"
  },
  "dependencies": {
    "@xyflow/react": "^12.8.4"
  },
  "peerDependencies": {
    "@mezzanine-ui/react": "*",
    "@mezzanine-ui/react-hook-form-v2": "*",
    "react": "*",
    "react-dom": "*",
    "react-hook-form": "*"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/react": "^9.1.3",
    "@storybook/react-vite": "^9.1.3",
    "storybook": "^9.1.3"
  }
}
