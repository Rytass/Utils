    "argon2": "*",
    "jsonwebtoken": "*",
    "typeorm": "*"
  },
  "dependencies": {
    "axios": "*",
    "casbin": "^5",
    "generate-password": "^1.7.1",
    "luxon": "^3.7.1",
    "typeorm-adapter": "^1"
  },
  "devDependencies": {
